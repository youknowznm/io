<% include ./common/page-header.ejs %>

<header class="gds-header" data-theme="blue">
    <div class="gds-header-content">
        <nav>
            <a class="site-title" href="javacript:;">
                <span class="single-word">Rh</span>
                <span class="single-word">ae</span>
                <span class="single-word">go</span>
            </a>
            <ul class="nav-items">
                <li class="nav-item active">blogs</li>
                <li class="nav-item">products</li>
                <li class="nav-item">messages</li>
                <li class="nav-item">about</li>
                <!-- <li class="nav-item search"></li> -->
                <li class="nav-indicator"></li>
            </ul>
        </nav>
        <div class="banner">
            <h1 class="page-title"><%= pageTitle %></h1>
        </div>
    </div>
    <div class="ripple"></div>
</header>
<div class="gds-header-shadow"></div>

<div class="main-wrap">

    <div class="row">
        <button class="md-button-flat" data-animating="false">
            <span class="content">button</span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-flat _primary" data-animating="false">
            <span class="content">primary</span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-flat _disabled" data-animating="false">
            <span class="content">disabled</span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-flat _warn" data-animating="false">
            <span class="content">warn</span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
    </div>
    <div class="row">
        <button class="md-button-round" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _primary" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _disabled" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _warn" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
    </div>
    <div class="row">
        <button class="md-button-round small" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _primary small" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _disabled small" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
        <button class="md-button-round _warn small" data-animating="false">
            <span class="content"></span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
    </div>

    <div class="row">
        <!-- 标题 -->
        <div class="md-input title">
            <label class="placeholder" for="input_1">Title</label>
            <input id="input_1" class="_input" maxlength="20" />
            <p class="error"></p>
            <h5 class="char-counter">
                <span class="current"></span>&nbsp;/&nbsp;<span class="maximum"></span>
            </h5>
        </div>
        <!-- 摘要 -->
        <div class="md-input summary">
            <label class="placeholder" for="input_2">Summary</label>
            <input id="input_2" class="_input" maxlength="40" />
            <p class="error"></p>
            <h5 class="char-counter">
                <span class="current"></span>&nbsp;/&nbsp;<span class="maximum"></span>
            </h5>
        </div>
    </div>
    <div class="row">
        <!-- 内容 -->
        <div class="md-textarea content">
            <label class="placeholder" for="input_4">Content (Markdown for blog. URL for product.)</label>
            <textarea id="input_4" class="_input" maxlength="500"></textarea>
            <p class="error"></p>
            <h5 class="char-counter">
                <span class="current"></span>&nbsp;/&nbsp;<span class="maximum"></span>
            </h5>
        </div>
    </div>
    <div class="row">
        <!-- 标签 -->
        <div class="md-chip tags">
            <label class="placeholder" for="input_3">Tags</label>
            <div class="chip-wrap">
                <!-- <span class="chip">Apple(fruit)<i class="btn-remove"></i></span> -->
                <input id="input_3" class="_input" maxlength="15" placeholder="Enter a tag..."/>
            </div>
            <p class="error">sb</p>
            <h5 class="char-counter">
                <span class="current"></span>&nbsp;/&nbsp;<span class="maximum"></span>
            </h5>
        </div>
        <!-- 提交按钮 -->
        <button class="md-button-flat submit" data-animating="false">
            <span class="content">submit</span>
            <div class="ripple-container"><span class="ripple"></span></div>
        </button>
    </div>


</div>

<% include ./common/page-footer.ejs %>
